{
	"name": "replicache-prisma-nextjs",
	"productName": "Replicache Demo using Prisma and NextJS",
	"version": "1.0.0",
	"description": "Replicache is a JavaScript framework for building high-performance, offline-capable, collaborative web apps.",
	"author": "Martin Adams <heymartinadams@users.noreply.github.com>",
	"private": false,
	"license": "MIT",
	"homepage": "./",
	"repository": {
		"url": "https://github.com/ConsciousApps/replicache-demo.git",
		"type": "git"
	},
	"scripts": {
		"dev": "next",
		"db:push": "npx prisma db push --schema=./prisma/schema.prisma --force-reset",
		"prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
		"prisma:studio": "prisma studio --schema=./prisma/schema.prisma"
	},
	"prisma": {
		"schema": "./prisma/schema.prisma"
	},
	"dependencies": {
		"@prisma/client": "^4.7.1",
		"cookies-next": "^2.1.1",
		"nanoid": "^3.3.4",
		"next": "^12.3.1",
		"pusher": "^5.1.1-beta",
		"pusher-js": "^7.4.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"replicache": "^12.0.1",
		"replicache-react": "^2.10.0"
	},
	"devDependencies": {
		"prisma": "^4.7.1"
	},
	"prettier": {
		"arrowParens": "avoid",
		"jsxSingleQuote": true,
		"printWidth": 100,
		"semi": false,
		"singleQuote": true,
		"trailingComma": "none",
		"useEditorConfig": false,
		"useTabs": true,
		"overrides": []
	}
}
